       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean class="City" name="city">
        <constructor-arg >
            <value>Moskva</value>
        </constructor-arg>
        <constructor-arg >
            <list>
                <value>1</value>
                <value>2</value>
            </list>
        </constructor-arg>
    </bean>
    <bean class="MyMap" name="map">
        <constructor-arg >


            <map>
                <entry key = "1" value = "INDIA"/>
                <entry key = "2" value = "CHINA"/>
                <entry key = "3" value = "RUSSIAo"/>
            </map>
        </constructor-arg>
    </bean>
